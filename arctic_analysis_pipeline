// Arctic SSHA Analysis Pipeline
digraph {
	rankdir=LR
	node [fillcolor=lightgrey fontname=Arial shape=box style="rounded,filled"]
	subgraph cluster_0 {
		label="Data Processing" style=rounded
		raw [label="ICESat-2
ATL21 Data"]
		prep [label="Preprocessed
SSHA Data"]
		raw -> prep
	}
	subgraph cluster_1 {
		label="Deep Learning Analysis" style=rounded
		cnn [label="CNN
Spatial Analysis"]
		lstm [label="LSTM
Temporal Analysis"]
	}
	subgraph cluster_2 {
		label=Results style=rounded
		pred [label="SSHA Predictions"]
		err [label="Error Maps"]
		temp [label="Temporal Patterns"]
		seas [label="Seasonal Trends"]
	}
	prep -> cnn
	prep -> lstm
	cnn -> pred
	cnn -> err
	lstm -> temp
	lstm -> seas
}
